{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/Orleans.StateMachineES/Orleans.StateMachineES.csproj",
            "src/Orleans.StateMachineES.Abstractions/Orleans.StateMachineES.Abstractions.csproj"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": false,
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "net9.0"
      }
    },
    {
      "src": [
        {
          "files": [
            "src/Orleans.StateMachineES.Generators/Orleans.StateMachineES.Generators.csproj"
          ],
          "src": ".."
        }
      ],
      "dest": "api",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "allowCompilationErrors": true,
      "filter": "filterConfig.yml",
      "properties": {
        "TargetFramework": "netstandard2.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "templates/custom"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Orleans.StateMachineES",
      "_appName": "Orleans.StateMachineES Documentation",
      "_appFooter": "Orleans.StateMachineES - Production-ready state machines for Microsoft Orleans",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_disableContribution": false,
      "_gitContribute": {
        "repo": "https://github.com/mivertowski/Orleans.StateMachineES",
        "branch": "main"
      },
      "_gitUrlPattern": "github"
    },
    "fileMetadata": {
      "_appLogoPath": {
        "**/*": "images/logo.svg"
      }
    }
  }
}
